<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>WOL</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")

</head>
<body>
    <div class="navbar navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                @Html.ActionLink("WOL", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Добавить компьютер", "Create", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Добавить пользователя", "CreateUser", "Home", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Выход", "Exit", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
    </div>
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>